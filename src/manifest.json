{
  //"update_url":"http://clients2.google.com/service/update2/crx",
  "name": "书迷",
  "version": "1.0.4",
  "manifest_version": 2,
  "description": "查看豆瓣、京东、亚马逊、当当网的图书在江苏大学图书馆借阅情况",
  "content_scripts": [ {
      "css": [ "inject.css" ],
      "js": [ "zepto.js" , "inject.js" ],
      "matches": ["http://book.douban.com/subject/*","http://www.amazon.cn/*","http://www.amazon.com/*",
        "http://product.dangdang.com/*", "http://item.jd.com/*"],
      "run_at": "document_end"
   } ],
  "options_page": "options.html",
  "background": {
    "persistent": false,
    "scripts": [ "zepto.js", "lib.js" , "background.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_title": "书迷",
    "default_popup": "popup.html"
  },
  "icons": {
    "128": "icon.png",
    "16": "icon.png",
    "32": "icon.png",
    "48": "icon.png"
  },
  "permissions": [
    "tabs",
    "http://*/*",
    "notifications"
  ],
  "web_accessible_resources": [ "inject.css" , "lib.js", "icon.png", "loading.gif"]
}